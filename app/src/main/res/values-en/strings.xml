    <string name="label_floating_write_paste">Copy recognition result to clipboard only</string>
    <string name="desc_floating_write_paste">Applies only to the packages below: in these apps, when using floating voice input, copy the result to the clipboard instead of inserting via Accessibility APIs. If a line contains all, it applies globally.</string>
    <string name="label_floating_write_paste_pkgs">Target packages (one per line)</string>
    <string name="hint_floating_write_paste_pkgs">Enter exact package names (e.g., com.tencent.mm). To apply globally, add a single line with all.</string>
